endpoint: ovh-eu  # ovh-eu, ovh-ca, ovh-us, or custom URL
application_key: your_application_key_here
application_secret: your_application_secret_here
consumer_key: your_consumer_key_here
timeout: 30  # seconds

# To generate these credentials:
# 1. Go to https://eu.api.ovh.com/createToken/
# 2. Set the following rights:
#    - GET /domain/zone/*
#    - POST /domain/zone/*
#    - PUT /domain/zone/*
#    - DELETE /domain/zone/*
# 3. Generate and save your credentials